  "scripts": {
    "chromatic": "npx chromatic --project-token=2763da0ce36c",
    "generate-graphql-ts-types": "graphql-codegen --config codegen.yml",
    "build": "yarn generate-graphql-ts-types && react-scripts build",
    "dev": "yarn generate-graphql-ts-types --watch & craco start",
    "test": "react-scripts test",
    "test:coverage": "yarn test --env=jsdom --coverage --watchAll=false",
    "upload:test-report": "./node_modules/.bin/codecov",
    "eject": "react-scripts eject",
    "tsc:check": "tsc --noEmit",
    "lint": "eslint --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --ignore-path ../.eslintignore",
    "lint:all": "yarn lint .",
    "lint:fix": "yarn lint --fix",
    "lint:fix:all": "yarn lint:fix .",
    "format:check:all": "yarn format:check \"src/**/*.{js,jsx,json,scss,md,ts,tsx}\" ",
    "format:check": "prettier --check --ignore-path ../.eslintignore",
    "format:write:all": "yarn format:write \"src/**/*.{js,jsx,json,scss,md,ts,tsx}\"",
    "format:write": "prettier --write --ignore-path ../.eslintignore",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "msw": {
    "workerDirectory": "public"
  }
}
