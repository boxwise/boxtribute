{
  "name": "@boxtribute/front",
  "version": "2.0.0",
  "private": true,
  "type": "module",
  "description": "Boxtribute v2 FE",
  "dependencies": {
    "@auth0/auth0-react": "^2.2.1",
    "@babel/runtime": "^7.20.13",
    "@boxtribute/shared-components": "1.0.0",
    "@fontsource/open-sans": "^4.5.14",
    "@fortawesome/fontawesome-svg-core": "^6.4.2",
    "@fortawesome/free-solid-svg-icons": "^6.4.2",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@sentry/integrations": "^7.72.0",
    "@sentry/react": "^7.72.0",
    "@zxing/browser": "^0.1.1",
    "@zxing/library": "^0.20.0",
    "chakra-react-select": "^4.4.3",
    "jwt-decode": "^3.1.2",
    "react-big-calendar": "^1.5.2",
    "react-icons": "^4.11.0",
    "react-table": "^7.8.0",
    "regenerator-runtime": "^0.14.0",
    "victory": "^36.6.8"
  },
  "devDependencies": {
    "@chakra-ui/storybook-addon": "^4.0.16",
    "@graphql-codegen/typescript": "^3.0.1",
    "@graphql-codegen/typescript-operations": "^3.0.4",
    "@graphql-codegen/typescript-react-query": "^4.1.0",
    "@sentry/types": "^7.72.0",
    "@storybook/addon-actions": "^7.3.2",
    "@storybook/addon-essentials": "^7.2.3",
    "@storybook/addon-interactions": "^7.5.3",
    "@storybook/addon-links": "^7.2.3",
    "@storybook/builder-webpack5": "^7.5.1",
    "@storybook/jest": "^0.2.3",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/node-logger": "^7.5.3",
    "@storybook/preset-create-react-app": "^4.1.2",
    "@storybook/react": "^8.1.11",
    "@storybook/testing-library": "^0.0.13",
    "@types/jwt-decode": "^3.1.0",
    "@types/react-big-calendar": "^0.38.2",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-table": "^7.7.14",
    "@types/w3c-image-capture": "^1.0.6",
    "autoprefixer": "^10.4.14",
    "babel-plugin-named-exports-order": "^0.0.2",
    "msw": "^1.1.0",
    "mutationobserver-shim": "^0.3.7",
    "prop-types": "^15.8.1"
  },
  "scripts": {
    "generate-graphql-ts-types": "graphql-codegen --config codegen.yml",
    "build": "yarn generate-graphql-ts-types && tsc && vite build",
    "dev": "yarn generate-graphql-ts-types --watch && vite ",
    "preview": "yarn generate-graphql-ts-types && vite preview",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "upload:test-report": "./node_modules/.bin/codecov",
    "tsc:check": "tsc --noEmit",
    "tsc:precommit": "tsc-files --noEmit",
    "lint": "eslint --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --ignore-path ../.eslintignore",
    "lint:all": "yarn lint .",
    "lint:fix": "yarn lint --fix",
    "lint:fix:all": "yarn lint:fix .",
    "format:check:all": "yarn format:check \"src/**/*.{js,jsx,json,scss,md,ts,tsx}\" ",
    "format:check": "prettier --check --ignore-path ../.eslintignore",
    "format:write:all": "yarn format:write \"src/**/*.{js,jsx,json,scss,md,ts,tsx}\"",
    "format:write": "prettier --write --ignore-path ../.eslintignore",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
